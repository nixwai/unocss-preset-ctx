{
  "name": "unocss-preset-ctx",
  "type": "module",
  "version": "0.2.3",
  "description": "Developed based on UnoCss. Make it easier to modify element styles in the context of CSS.",
  "author": "nixwai <https://github.com/nixwai>",
  "license": "MIT",
  "homepage": "https://nixwai.github.io/unocss-preset-ctx/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nixwai/unocss-preset-ctx"
  },
  "keywords": [
    "css",
    "unocss",
    "unocss preset"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./helper": {
      "types": "./types/helper.d.ts",
      "import": "./dist/helper.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "@unocss/preset-mini": "catalog:",
    "magic-color": "catalog:"
  },
  "devDependencies": {
    "unocss": "catalog:"
  }
}
